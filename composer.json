{
    "name": "axguowen/think-orm",
    "description": "think orm For PHP5.6",
    "keywords": [
        "orm",
        "database"
    ],      
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "axguowen",
            "email": "axguowen@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "psr/simple-cache": "^1.0",
        "psr/log": "^1.0",
        "axguowen/think-helper":"^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "think\\": "src"
        },
        "files": [
            "stubs/load_stubs.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
